pipeline{
    agent any
    stages{
        stage("First job"){
            steps{
                sh '''
                        echo "This is the first job"
                   '''
            }
        }
        
        stage("Second job"){
            steps{
                sh '''
                        echo "This is the second job to show its working. "Added Jenkins"
                   '''
            }
        }
    
    }
}